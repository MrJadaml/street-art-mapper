.col-md-12
  .row
    .col-md-7#gallery-map.gallery-map
    .gallery
      .scrollable
        -@murals.each do |mural|
          .col-md-4.gallery-mural
            .mapPin{:class => mural.id}
              = link_to mural do
                = image_tag mural.image.url(:user_ablum)
- content_for(:js) do
  :javascript
    $(function(){
      MapFunctions.galleryMap();
    });
